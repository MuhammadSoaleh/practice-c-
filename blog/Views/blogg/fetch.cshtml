@model IEnumerable<blogs.Models.blogg>;
@foreach(var item in Model){
   <div class="card mb-3">
  
  <div class="card-body">
    <h5 class="card-title">@item.title</h5>
    <h5 class="card-title">@item.author</h5>
    <p class="card-text">@item.description.</p>
    <p class="card-text"><small class="text-body-secondary">@item.content</small></p>
    <p class="card-text"><small class="text-body-secondary">@item.subheading</small></p>
            <a class="btn btn-primary" asp-action="Fetchblog" asp-controller="blogg" asp-route-id=@item.id>read full blog</a>
            @* <a class="btn btn-primary" asp-action="edit" asp-route-id=@item.id>EDIT</a>  *@
            <a class="btn btn-danger" asp-action="delete" asp-route-id="@item.id">DELETE</a>
  </div>
        <img src="~/@item.image" class="card-img-top" alt="...">
</div>

}

