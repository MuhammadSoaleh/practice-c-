@model IEnumerable<blogs.Models.blogg>;
@foreach(var item in Model){
   <div class="card mb-3">
  
  <div class="card-body">
    <h5 class="card-title">@item.title</h5>
    <h5 class="card-title">@item.author</h5>
    <p class="card-text">@item.description.</p>
    <p class="card-text"><small class="text-body-secondary">@item.content</small></p>
    <p class="card-text"><small class="text-body-secondary">@item.subheading</small></p>
  </div>
        <img src="~/@item.image" class="card-img-top" alt="...">
</div>

}

